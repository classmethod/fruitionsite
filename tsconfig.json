    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": [
            "dom",
            "esnext"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "jsx": "react",
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks": true
